{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RestCountries": {
    "APIURL": "https://restcountries.eu/rest/v2/",
    "TimeToCacheInMinutes": "1440"
  },
  "DefaultHttpResiliencePolicy": {
    "RetryCount": 3,
    "RetryWait": 200,
    "TimeoutSeconds": 10
  }
}
